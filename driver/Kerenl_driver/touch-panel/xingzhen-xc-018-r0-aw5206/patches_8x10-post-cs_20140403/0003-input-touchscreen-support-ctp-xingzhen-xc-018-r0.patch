+static ssize_t AW5306_store_enable(struct device *dev,
+		struct device_attribute *attr,
+		const char *buf, size_t size)
+{
+	struct AW5306_ts_data *data = dev_get_drvdata(dev);
+	unsigned long val;
+	int rc;
+
+	if (size > 2)
+		return -EINVAL;
+
+	if (suspend_flag) {
+		dev_info(&data->client->dev, "Already in suspend state\n");
+		goto no_operation;
+	}
+
+	rc = kstrtoul(buf, 10, &val);
+	if (rc != 0)
+		return rc;
+
+	if (val) {
+		AW5306_TP_Reinit();
+		enable_irq(this_client->irq);
+		data->enable = true;
+	} else {
+		disable_irq(this_client->irq);
+		AW5306_Sleep();
+		data->enable = false;
+	}
+
+no_operation:
+	return size;
+}
