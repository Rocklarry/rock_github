+static int AW5306_ts_suspend(struct device *dev)
+{
+#ifdef INTMODE
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+#endif
+
+	if (suspend_flag != 1) {
+		TS_DBG("==AW5306_ts_suspend=\n");
+		suspend_flag = 1;
+		AW_Sleep(50);
+#ifdef INTMODE
+		disable_irq(this_client->irq);
+		AW5306_Sleep();
+		AW5306_power_on(data, false);
+#endif
+
+	}
+	return 0;
+}
+
+static int AW5306_ts_resume(struct device *dev)
+{
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+
+	if (suspend_flag != 0) {
+		AW5306_power_on(data, true);
+		msleep(20);
+		gpio_set_value_cansleep(data->pdata->reset_gpio, 0);
+		msleep(20);
+		gpio_set_value_cansleep(data->pdata->reset_gpio, 1);
+		msleep(30);
+		gpio_set_value_cansleep(data->pdata->reset_gpio, 0);
+		msleep(20);
+
+#ifdef INTMODE
+		AW5306_TP_Init();
+		msleep(20);
+		AW5306_TP_Reinit();
+
+
+		enable_irq(this_client->irq);
+		suspend_flag = 0;
+		TS_DBG("AW5306 WAKE UP!!!");
+#else
+		AW5306_TP_Reinit();
+		tp_idlecnt = 0;
+		tp_SlowMode = 0;
+		suspend_flag = 0;
+		TS_DBG("AW5306 WAKE UP!!!");
+		data->touch_timer.expires = jiffies + HZ/AWTPCfg.FAST_FRAME;
+		add_timer(&data->touch_timer);
+#endif
+	}
+	return 0;
+}
