+static const struct dev_pm_ops AW5306_ts_pm_ops = {
+#if (!defined(CONFIG_FB) && !defined(CONFIG_HAS_EARLYSUSPEND))
+	.suspend = AW5306_ts_suspend,
+	.resume = AW5306_ts_resume,
+#endif
+};
+
+static int AW5306_ts_probe(struct i2c_client *client,
+		const struct i2c_device_id *id)
+{
+	struct AW5306_ts_platform_data *pdata;
+	struct AW5306_ts_data *AW5306_ts;
+	struct input_dev *input_dev;
+	struct dentry *temp;
+	int err, len;
+	unsigned char reg_value;
+
+	debug_level = 0;
+
+	TS_DBG("==AW5306_ts_probe=\n");
+
+	if (client->dev.of_node) {
+		pdata = devm_kzalloc(&client->dev,
+				sizeof(struct AW5306_ts_platform_data),
+				GFP_KERNEL);
+		if (!pdata) {
+			dev_err(&client->dev, "Failed to allocate memory\n");
+			err = -ENOMEM;
+			goto exit_check_pdata_failed;
+		}
+
+		err = aw5306_parse_dt(&client->dev, pdata);
+		if (err) {
+			dev_err(&client->dev, "DT parsing failed\n");
+			goto exit_parse_dt_failed;
+		}
+	} else
+		pdata = client->dev.platform_data;
+
+	if (!pdata) {
+		dev_err(&client->dev, "Invalid pdata\n");
+		err =  -EINVAL;
+		goto exit_check_pdata_failed;
+	}
+
+	if (!i2c_check_functionality(client->adapter, I2C_FUNC_I2C)) {
+		err = -ENODEV;
+		goto exit_check_functionality_failed;
+	}
+
+	TS_DBG("==kzalloc=");
+	AW5306_ts = kzalloc(sizeof(*AW5306_ts), GFP_KERNEL);
+	if (!AW5306_ts)	{
+		err = -ENOMEM;
+		goto exit_alloc_data_failed;
+	}
+
+	if (pdata->fw_name) {
+		len = strlen(pdata->fw_name);
+		if (len > AW_FW_NAME_MAX_LEN - 1) {
+			dev_err(&client->dev, "Invalid firmware name\n");
+			err = -EINVAL;
+			goto exit_invalid_fw_name;
+		}
+
+		strlcpy(AW5306_ts->fw_name, pdata->fw_name, len + 1);
+	}
+
+	AW5306_ts->client = client;
+	AW5306_ts->pdata = pdata;
+
+	err = aw5306_power_init(AW5306_ts, true);
+	if (err) {
+		dev_err(&client->dev, "power init failed");
+		goto exit_init_power_failed;
+	}
+
+	if (pdata->power_on) {
+		err = pdata->power_on(true);
+		if (err) {
+			dev_err(&client->dev, "power on failed");
+			goto exit_power_on_failed;
+		}
+	} else {
+		err = AW5306_power_on(AW5306_ts, true);
+		if (err) {
+			dev_err(&client->dev, "power on failed");
+			goto exit_power_on_failed;
+		}
+	}
+
+	if (gpio_is_valid(pdata->irq_gpio)) {
+		err = gpio_request(pdata->irq_gpio, "AW5306_irq_gpio");
+		if (err) {
+			dev_err(&client->dev, "irq gpio request failed");
+			goto exit_req_irq_gpio_failed;
+		}
+		err = gpio_direction_input(pdata->irq_gpio);
+		if (err) {
+			dev_err(&client->dev,
+					"set_direction for irq gpio failed\n");
+			goto exit_set_irq_gpio_failed;
+		}
+	}
+
+	if (gpio_is_valid(pdata->reset_gpio)) {
+		err = gpio_request(pdata->reset_gpio, "AW5306_reset_gpio");
+		if (err) {
+			dev_err(&client->dev, "reset gpio request failed");
+			goto exit_req_reset_gpio_failed;
+		}
+
+		err = gpio_direction_output(pdata->reset_gpio, 1);
+		if (err) {
+			dev_err(&client->dev, "set_direction for reset gpio failed\n");
+			goto exit_set_reset_gpio_failed;
+		}
+		msleep(20);
+		gpio_set_value_cansleep(pdata->reset_gpio, 0);
+	}
+
+	client->addr = AW5306_TS_ADDR;
+	i2c_set_clientdata(client, AW5306_ts);
+
+	client->irq = gpio_to_irq(pdata->irq_gpio);
+	msleep(20);
+	TS_DBG("%s IRQ number is %d", client->name, client->irq);
+	this_client = client;
+	TS_DBG("I2C addr=%x", client->addr);
+	reg_value = AW_I2C_ReadByte(0x01);
+	TS_DBG("AW5306 chip ID = 0x%x vincent***", reg_value);
+
+	if (reg_value != 0xA8) {
+		err = -ENODEV;
+		dev_err(&client->dev, "Read chip ID failed!\n");
+		goto exit_chip_check_id_failed;
+	}
+
+	AW5306_ts->enable = true;
+	AW5306_ts->device_id = reg_value;
+	AW5306_ts->num_max_touches = MAX_POINT;
+	AW5306_ts->fw_ver = 20130816;
+
+	suspend_flag = 0;
+	tp_idlecnt = 0;
+	tp_SlowMode = 1;
+
+
+	INIT_WORK(&AW5306_ts->pen_event_work, AW5306_ts_work);
+
+	AW5306_ts->ts_workqueue =
+		create_singlethread_workqueue(dev_name(&client->dev));
+	if (!AW5306_ts->ts_workqueue) {
+		err = -ESRCH;
+		goto exit_create_singlethread;
+	}
+#ifdef INTMODE
+	TS_DBG("%s IRQ number is %d", client->name, client->irq);
+/*	gpio_tlmm_config(GPIO_CFG(17, 0,
+				GPIO_CFG_INPUT, GPIO_CFG_PULL_UP,
+				GPIO_CFG_8MA), GPIO_CFG_ENABLE);
+*/
+	err = request_threaded_irq(client->irq, AW5306_ts_interrupt, NULL,
+			IRQF_TRIGGER_LOW, client->name, AW5306_ts);
+	if (err < 0) {
+		dev_err(&client->dev, "AW5306_probe: request irq failed\n");
+		goto exit_req_irq_failed;
+	}
+	disable_irq(client->irq);
+#endif
+	input_dev = input_allocate_device();
+	if (!input_dev) {
+		err = -ENOMEM;
+		dev_err(&client->dev, "failed to allocate input device\n");
+		goto exit_allocate_dev_failed;
+	}
+
+	AW5306_ts->input_dev = input_dev;
+
+	set_bit(ABS_MT_TOUCH_MAJOR, input_dev->absbit);
+	set_bit(ABS_MT_POSITION_X, input_dev->absbit);
+	set_bit(ABS_MT_POSITION_Y, input_dev->absbit);
+
+	set_bit(KEY_MENU,  input_dev->keybit);
+	set_bit(KEY_BACK,  input_dev->keybit);
+	set_bit(KEY_HOME,  input_dev->keybit);
+	set_bit(KEY_SEARCH,  input_dev->keybit);
+
+	set_bit(EV_KEY, input_dev->evbit);
+	set_bit(EV_ABS, input_dev->evbit);
+/*	set_bit(BTN_TOUCH, input_dev->keybit);
+ */
+	set_bit(INPUT_PROP_DIRECT, input_dev->propbit);
+	input_set_abs_params(input_dev, ABS_MT_POSITION_X, 0,
+			SCREEN_MAX_X, 0, 0);
+	input_set_abs_params(input_dev, ABS_MT_POSITION_Y, 0,
+			SCREEN_MAX_Y, 0, 0);
+	input_set_abs_params(input_dev, ABS_MT_TOUCH_MAJOR, 0, PRESS_MAX, 0, 0);
+	input_mt_init_slots(input_dev, 5);
+
+	input_dev->name	= AW5306_TS_NAME;
+	err = input_register_device(input_dev);
+	if (err) {
+		dev_err(&client->dev,
+		" luke: AW5306_ts_probe: failed to register input device: %s\n",
+		dev_name(&client->dev));
+		goto exit_input_register_device_failed;
+	}
+
+#if defined(CONFIG_FB)
+	AW5306_ts->fb_notif.notifier_call = fb_notifier_callback;
+
+	err = fb_register_client(&AW5306_ts->fb_notif);
+	if (err)
+		dev_err(&client->dev, "Unable to register fb_notifier: %d\n",
+				err);
+#elif defined(CONFIG_HAS_EARLYSUSPEND)
+	AW5306_ts->early_suspend.level = EARLY_SUSPEND_LEVEL_BLANK_SCREEN + 1;
+	AW5306_ts->early_suspend.suspend = AW5306_ts_suspend;
+	AW5306_ts->early_suspend.resume	= AW5306_ts_resume;
+	register_early_suspend(&AW5306_ts->early_suspend);
+#endif
+	msleep(20);
+
+	AW5306_create_sysfs(client);
+
+	AW5306_ts->dir = debugfs_create_dir(DEBUG_DIR_NAME, NULL);
+	if (AW5306_ts->dir == NULL || IS_ERR(AW5306_ts->dir)) {
+		pr_err("debugfs_create_dir failed(%ld)\n",
+				PTR_ERR(AW5306_ts->dir));
+		err = PTR_ERR(AW5306_ts->dir);
+		goto exit_create_debugfs_dir_failed;
+	}
+	temp = debugfs_create_file("addr", S_IRUSR | S_IWUSR, AW5306_ts->dir,
+			AW5306_ts, &debug_addr_fops);
+	if (temp == NULL || IS_ERR(temp)) {
+		pr_err("debugfs_create_file failed: rc=%ld\n", PTR_ERR(temp));
+		err = PTR_ERR(temp);
+		goto exit_create_debugfs_file_failed;
+	}
+
+	temp = debugfs_create_file("data", S_IRUSR | S_IWUSR, AW5306_ts->dir,
+			AW5306_ts, &debug_data_fops);
+	if (temp == NULL || IS_ERR(temp)) {
+		pr_err("debugfs_create_file failed: rc=%ld\n", PTR_ERR(temp));
+		err = PTR_ERR(temp);
+		goto exit_create_debugfs_file_failed;
+	}
+
+	temp = debugfs_create_file("suspend", S_IRUSR | S_IWUSR,
+			AW5306_ts->dir, AW5306_ts, &debug_suspend_fops);
+	if (temp == NULL || IS_ERR(temp)) {
+		pr_err("debugfs_create_file failed: rc=%ld\n", PTR_ERR(temp));
+		err = PTR_ERR(temp);
+		goto exit_create_debugfs_file_failed;
+	}
+
+	temp = debugfs_create_file("dump_info", S_IRUSR | S_IWUSR,
+			AW5306_ts->dir, AW5306_ts, &debug_dump_info_fops);
+	if (temp == NULL || IS_ERR(temp)) {
+		pr_err("debugfs_create_file failed: rc=%ld\n", PTR_ERR(temp));
+		err = PTR_ERR(temp);
+		goto exit_create_debugfs_file_failed;
+	}
+
+
+	memcpy(AW_CALI_FILENAME, "/data/tpcali", 12);
+	memcpy(AW_UCF_FILENAME, "/data/AWTPucf", 13);
+
+	AW5306_TP_Init();
+	msleep(20);
+	AW5306_TP_Reinit();
+
+	AW5306_ts->touch_timer.function = AW5306_tpd_polling;
+	AW5306_ts->touch_timer.data = 0;
+	init_timer(&AW5306_ts->touch_timer);
+	AW5306_ts->touch_timer.expires = jiffies + HZ*5;
+	add_timer(&AW5306_ts->touch_timer);
+
+
+	AW5306_ts->ts_info = devm_kzalloc(&client->dev,
+			AW_INFO_MAX_LEN, GFP_KERNEL);
+	if (!AW5306_ts->ts_info) {
+		dev_err(&client->dev, "Not enough memory\n");
+		goto exit_alloc_ts_info_mem_failed;
+	}
+
+	AW_STORE_TS_INFO(AW5306_ts->ts_info, AW5306_ts->device_id, pdata->name,
+			AW5306_ts->num_max_touches,
+			pdata->fw_vkey_support ? "yes" : "no",
+			pdata->fw_name, AW5306_ts->fw_ver);
+
+
+#ifdef INTMODE
+/*	AW_I2C_WriteByte(0x02, 0x09);
+	AW_I2C_WriteByte(0x03, 0x01);
+*/
+	enable_irq(client->irq);
+#endif
+	TS_DBG("==probe over =\n");
+	return 0;
+
+exit_alloc_ts_info_mem_failed:
+exit_create_debugfs_file_failed:
+	debugfs_remove_recursive(AW5306_ts->dir);
+exit_create_debugfs_dir_failed:
+	input_unregister_device(input_dev);
+exit_input_register_device_failed:
+	input_free_device(input_dev);
+exit_allocate_dev_failed:
+	free_irq(client->irq, AW5306_ts);
+exit_req_irq_failed:
+	cancel_work_sync(&AW5306_ts->pen_event_work);
+exit_create_singlethread:
+	destroy_workqueue(AW5306_ts->ts_workqueue);
+exit_chip_check_id_failed:
+	i2c_set_clientdata(client, NULL);
+exit_set_reset_gpio_failed:
+	if (gpio_is_valid(pdata->reset_gpio))
+		gpio_free(pdata->reset_gpio);
+exit_req_reset_gpio_failed:
+exit_set_irq_gpio_failed:
+	if (gpio_is_valid(pdata->irq_gpio))
+		gpio_free(pdata->irq_gpio);
+exit_req_irq_gpio_failed:
+	AW5306_power_on(AW5306_ts, false);
+exit_power_on_failed:
+	aw5306_power_init(AW5306_ts, false);
+exit_init_power_failed:
+exit_invalid_fw_name:
+	kfree(AW5306_ts);
+exit_alloc_data_failed:
+exit_check_functionality_failed:
+exit_parse_dt_failed:
+exit_check_pdata_failed:
+	return err;
+}
+
+static int __devexit AW5306_ts_remove(struct i2c_client *client)
+{
+
+	struct AW5306_ts_data *AW5306_ts = i2c_get_clientdata(client);
+
+	TS_DBG("==AW5306_ts_remove=\n");
+
+	suspend_flag = 1;
+	AW_Sleep(50);
+
+#if defined(CONFIG_FB)
+	if (fb_unregister_client(&AW5306_ts->fb_notif))
+		dev_err(&client->dev, "Error occurred while unregistering fb_notifier.\n");
+#elif defined(CONFIG_HAS_EARLYSUSPEND)
+	unregister_early_suspend(&AW5306_ts->early_suspend);
+#endif
+	input_unregister_device(AW5306_ts->input_dev);
+
+	free_irq(client->irq, AW5306_ts);
+
+	cancel_work_sync(&AW5306_ts->pen_event_work);
+	destroy_workqueue(AW5306_ts->ts_workqueue);
+
+	i2c_set_clientdata(client, NULL);
+
+	if (gpio_is_valid(AW5306_ts->pdata->reset_gpio))
+		gpio_free(AW5306_ts->pdata->reset_gpio);
+
+	if (gpio_is_valid(AW5306_ts->pdata->irq_gpio))
+		gpio_free(AW5306_ts->pdata->irq_gpio);
+
+	AW5306_power_on(AW5306_ts, false);
+	aw5306_power_init(AW5306_ts, false);
+
+	kfree(AW5306_ts);
+
+	return 0;
+}
+
+#ifdef CONFIG_OF
+static struct of_device_id aw5206_match_table[] = {
+	{ .compatible = "awin,aw5206",},
+	{ },
+};
+#else
+#define aw5206_match_table NULL
+#endif
+static const struct i2c_device_id AW5306_ts_id[] = {
+	{ AW5306_TS_NAME, 0 },
+	{ }
+};
+
+
+MODULE_DEVICE_TABLE(i2c, AW5306_ts_id);
+
+static struct i2c_driver AW5306_ts_driver = {
+	.probe		= AW5306_ts_probe,
+	.remove		= __devexit_p(AW5306_ts_remove),
+	.id_table	= AW5306_ts_id,
+	.driver	= {
+		.name	= AW5306_TS_NAME,
+		.owner	= THIS_MODULE,
+		.of_match_table = aw5206_match_table,
+#ifdef CONFIG_PM
+		.pm = &AW5306_ts_pm_ops,
+#endif
+	},
+};
+
+static int __init AW5306_ts_init(void)
+{
+	int ret;
+	TS_DBG("luke:   %s    %d\n", __func__, __LINE__);
+	ret = i2c_add_driver(&AW5306_ts_driver);
+	return ret;
+}
+
+static void __exit AW5306_ts_exit(void)
+{
+	TS_DBG("==AW5306_ts_exit==\n");
+	i2c_del_driver(&AW5306_ts_driver);
+}
+
+module_init(AW5306_ts_init);
+module_exit(AW5306_ts_exit);
+
+MODULE_AUTHOR("<wuhaijun@AWINIC.com>");
+MODULE_DESCRIPTION("AWINIC AW5306 TouchScreen driver");
+MODULE_LICENSE("GPL");
