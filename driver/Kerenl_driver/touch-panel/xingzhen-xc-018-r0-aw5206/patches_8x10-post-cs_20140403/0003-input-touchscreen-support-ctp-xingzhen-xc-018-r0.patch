+		enable_irq(this_client->irq);
+		suspend_flag = 0;
+#else
+		suspend_flag = 1;
+		AW_Sleep(50);
+
+		FreqScan(Basefreq);
+
+		AW5306_TP_Reinit();
+		tp_idlecnt = 0;
+		tp_SlowMode = 0;
+		suspend_flag = 0;
+		data->touch_timer.expires = jiffies + HZ/AWTPCfg.FAST_FRAME;
+		add_timer(&data->touch_timer);
+#endif
+	}
