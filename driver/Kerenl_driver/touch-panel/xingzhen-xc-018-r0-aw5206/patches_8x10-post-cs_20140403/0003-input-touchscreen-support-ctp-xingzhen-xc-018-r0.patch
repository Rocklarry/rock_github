+static ssize_t AW5306_write_reg(struct device *cd,
+		struct device_attribute *attr,
+		const char *buf, size_t len)
+{
+	int databuf[2];
+
+	if (2 == sscanf(buf, "%d %d", &databuf[0], &databuf[1])) {
+		if (suspend_flag != 1) {
+#ifdef INTMODE
+			disable_irq(this_client->irq);
+			AW5306_Sleep();
+			suspend_flag = 1;
+			AW_Sleep(50);
+
+			AW_I2C_WriteByte(databuf[0], databuf[1]);
+
+			AW5306_TP_Reinit();
+			enable_irq(this_client->irq);
+			suspend_flag = 0;
+#else
+			suspend_flag = 1;
+			AW_Sleep(50);
+
+			AW_I2C_WriteByte((u8)databuf[0], (u8)databuf[1]);
+
+			AW5306_TP_Reinit();
+			tp_idlecnt = 0;
+			tp_SlowMode = 0;
+			suspend_flag = 0;
+			data->touch_timer.expires =
+				jiffies + HZ/AWTPCfg.FAST_FRAME;
+			add_timer(&data->touch_timer);
+#endif
+		} else
+			AW_I2C_WriteByte(databuf[0], databuf[1]);
+	}
+
+	return len;
+}
