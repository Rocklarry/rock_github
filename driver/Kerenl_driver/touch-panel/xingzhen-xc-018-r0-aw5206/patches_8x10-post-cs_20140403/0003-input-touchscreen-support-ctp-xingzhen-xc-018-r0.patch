+void AW5306_tpd_polling(unsigned long unuse)
+{
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+
+#ifdef INTMODE
+	return ;
+#endif
+	if (!work_pending(&data->pen_event_work))
+		queue_work(data->ts_workqueue, &data->pen_event_work);
+	if (suspend_flag != 1) {
+#ifdef AUTO_REDUCEFRAME
+		if (tp_SlowMode)
+			data->touch_timer.expires =
+				jiffies + HZ/AWTPCfg.SLOW_FRAME;
+		else
+			data->touch_timer.expires =
+				jiffies + HZ/AWTPCfg.FAST_FRAME;
+#else
+		data->touch_timer.expires = jiffies + HZ/AWTPCfg.FAST_FRAME;
+#endif
+		add_timer(&data->touch_timer);
+	}
+}
