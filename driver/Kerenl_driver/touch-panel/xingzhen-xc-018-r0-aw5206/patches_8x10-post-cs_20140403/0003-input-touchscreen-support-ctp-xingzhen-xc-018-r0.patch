+static ssize_t AW5306_set_cali(struct device *cd, struct device_attribute *attr,
+		const char *buf, size_t len)
+{
+	int rc;
+#ifndef INTMODE
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+#endif
+/*	unsigned long on_off = simple_strtoul(buf, NULL, 10);
+ */
+	unsigned long on_off;
+
+	rc = kstrtoul(buf, 10, &on_off);
+	if (rc != 0)
+		return rc;
+
+	if (on_off == 1) {
+#ifdef INTMODE
+		disable_irq(this_client->irq);
+		AW5306_Sleep();
+		suspend_flag = 1;
+		AW_Sleep(50);
+
+		TP_Force_Calibration();
+
+		AW5306_TP_Reinit();
+		enable_irq(this_client->irq);
+		suspend_flag = 0;
+#else
+		suspend_flag = 1;
+		AW_Sleep(50);
+
+		TP_Force_Calibration();
+
+		AW5306_TP_Reinit();
+		tp_idlecnt = 0;
+		tp_SlowMode = 0;
+		suspend_flag = 0;
+		data->touch_timer.expires = jiffies + HZ/AWTPCfg.FAST_FRAME;
+		add_timer(&data->touch_timer);
+#endif
+	}
+
+	return len;
+}
