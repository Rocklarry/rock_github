+static int AW5306_read_data(void)
+{
+	u16 i;
+	int j;
+	int Pevent;
+	int down_table = 0;
+	static int flag_realse = 2;
+
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+	struct ts_event *event = &data->event;
+	struct cur_event *cevt = &data->cevt;
+
+	AW5306_TouchProcess();
+
+	event->touch_point = AW5306_GetPointNum();
+
+/*	TS_DBG("AW5306 Touch_point = %d\n", event->touch_point);
+ */
+
+	for (i = 0; i < event->touch_point; i++) {
+		AW5306_GetPoint(&event->x[i], &event->y[i], &event->id[i],
+				&Pevent, i);
+
+		if (AWTPCfg.DEBUG_LEVEL > 0)
+			printk(KERN_INFO "AW5306 point %d = %d, y=%d, id = %d!!!!!\n",
+				i, event->x[i], event->y[i], event->id[i]);
+
+	}
+
+	for (j = 5 - event->touch_point; j > 0; j--)
+		event->id[j + event->touch_point - 1] = -1;
+
+	for (j = 0; j < 5; j++)
+		cevt->id[j] = -1;
+
+	if (event->touch_point > 0) {
+		for (j = 0; j < 5; j++) {
+			if (event->id[j] != -1) {
+				cevt->id[event->id[j]] = event->id[j];
+				cevt->x[event->id[j]] = event->x[j];
+				cevt->y[event->id[j]] = event->y[j];
+
+				down_table |= (1 << cevt->id[event->id[j]]);
+			}
+		}
+	}
+
+	if (event->touch_point == 0) {
+		if (tp_idlecnt <= AWTPCfg.FAST_FRAME*10)
+			tp_idlecnt++;
+		if (tp_idlecnt > AWTPCfg.FAST_FRAME*10)
+			tp_SlowMode = 1;
+		if (event->pre_point != 0) {
+			AW5306_ts_release();
+			if (flag_realse-- == 0)
+				event->pre_point = 0;
+		}
+		/*	return 1;
+		 */
+	}
+	/*	else
+	 */
+	{
+		flag_realse = 2;
+		tp_SlowMode = 0;
+		tp_idlecnt = 0;
+		event->pre_point = event->touch_point;
+		event->pressure = 200;
+
+		for (j = 0; j < 5; j++) {
+			input_mt_slot(data->input_dev, j);
+			if (down_table & (1 << j)) {
+				input_mt_report_slot_state(data->input_dev,
+						MT_TOOL_FINGER, 1);
+				input_report_abs(data->input_dev,
+						ABS_MT_POSITION_X,
+						cevt->x[j]);
+				input_report_abs(data->input_dev,
+						ABS_MT_POSITION_Y,
+						cevt->y[j]);
+			} else {
+				input_mt_report_slot_state(data->input_dev,
+						MT_TOOL_FINGER, 0);
+			}
+		}
+		input_sync(data->input_dev);
+
+
+		return 0;
+	}
+}
