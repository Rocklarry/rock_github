+static ssize_t AW5306_get_reg(struct device *cd,
+		struct device_attribute *attr, char *buf)
+{
+#ifndef INTMODE
+	struct AW5306_ts_data *data = i2c_get_clientdata(this_client);
+#endif
+	u8 reg_val[128];
+	ssize_t len = 0;
+	u8 i;
+
+	if (suspend_flag != 1) {
+#ifdef INTMODE
+		disable_irq(this_client->irq);
+		AW5306_Sleep();
+		suspend_flag = 1;
+		AW_Sleep(50);
+
+		AW_I2C_ReadXByte(reg_val, 0, 127);
+
+		AW5306_TP_Reinit();
+		enable_irq(this_client->irq);
+		suspend_flag = 0;
+#else
+		suspend_flag = 1;
+
+		AW_Sleep(50);
+
+		AW_I2C_ReadXByte(reg_val, 0, 127);
+
+		AW5306_TP_Reinit();
+		tp_idlecnt = 0;
+		tp_SlowMode = 0;
+		suspend_flag = 0;
+		data->touch_timer.expires =
+			jiffies + HZ/AWTPCfg.FAST_FRAME;
+		add_timer(&data->touch_timer);
+#endif
+	} else
+		AW_I2C_ReadXByte(reg_val, 0, 127);
+
+	for (i = 0; i < 0x7F; i++)
+		len += snprintf(buf+len, PAGE_SIZE-len,
+				"reg%02X = 0x%02X, ", i, reg_val[i]);
+
+	return len;
+
+}
