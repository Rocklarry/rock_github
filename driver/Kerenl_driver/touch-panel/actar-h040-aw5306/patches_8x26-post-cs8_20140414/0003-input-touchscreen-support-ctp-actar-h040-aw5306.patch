+static void AW5306_ts_work(struct work_struct *work)
+{
+	int ret = -1;
+
+	ret = AW5306_read_data();
+
+#ifdef INTMODE
+	if (suspend_flag != 1)
+		enable_irq(this_client->irq);
+#else
+	if (suspend_flag == 1)
+		AW5306_Sleep();
+#endif
+}
