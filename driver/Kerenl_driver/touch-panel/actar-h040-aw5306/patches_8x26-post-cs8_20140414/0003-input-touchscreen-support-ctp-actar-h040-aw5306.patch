+	unsigned char	PointPressCnt[MAX_POINT];
+
+};
+
+struct STRUCTCALI {
+	unsigned char fileflag[14];
+	unsigned char TXOFFSET[(NUM_TX+1)/2];
+	unsigned char RXOFFSET[(NUM_RX+1)/2];
