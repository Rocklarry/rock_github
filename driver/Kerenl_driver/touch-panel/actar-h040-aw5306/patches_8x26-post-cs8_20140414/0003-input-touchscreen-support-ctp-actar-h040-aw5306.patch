+struct STRUCTPOINT {
+	unsigned short	X, Y;
+
+	unsigned int	PointID;
+	unsigned char	Event;
+};
+
+struct STRUCTFRAME {
+	struct STRUCTPOINT	PointInfo[MAX_POINT];
+	struct STRUCTPOINT		RptPoint[MAX_POINT];
+	unsigned char	PointNum;
+	unsigned char	LastPointNum;
+	unsigned char	NegPointNum;
