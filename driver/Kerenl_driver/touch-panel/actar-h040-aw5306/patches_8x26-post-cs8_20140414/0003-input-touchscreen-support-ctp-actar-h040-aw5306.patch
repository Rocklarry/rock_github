+static ssize_t AW5x0x_SetTest(struct device *cd,
+			struct device_attribute *attr,
+			const char *buf, size_t count)
+{
+#ifdef INTMODE
+#else
+	struct AW5x0x_ts_data *data = i2c_get_clientdata(this_client);
+#endif
+	int rc;
+	unsigned long on_off;
+	rc = kstrtoul(buf, 10, &on_off);
+	AWTP_test_st = 0;
+
+	if (on_off == 1) {
+#ifdef INTMODE
+		ctp_disable_irq();
+		AW5x0x_Sleep();
+		suspend_flag = 1;
+		AW_Sleep(50);
+		AW5x0x_TP_Test();
+
+		AW5x0x_TP_Reinit();
+		ctp_enable_irq();
+		suspend_flag = 0;
+#else
+		suspend_flag = 1;
+		AW_Sleep(50);
+		AW5x0x_TP_Test();
+
+		AW5x0x_TP_Reinit();
+		tp_idlecnt = 0;
+		tp_SlowMode = 0;
+		suspend_flag = 0;
+
+		hrtimer_start(&data->touch_timer,
+			ktime_set(0, (1000 / AWTPCfg.FAST_FRAME) * 1000000),
+			HRTIMER_MODE_REL);
+
+#endif
+	}
+
+	AWTP_test_st = 1;
+
+	return count;
+}
