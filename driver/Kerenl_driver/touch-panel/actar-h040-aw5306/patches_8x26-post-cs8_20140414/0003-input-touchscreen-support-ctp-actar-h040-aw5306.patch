+static int aw5306_parse_dt(struct device *dev,
+		struct AW5306_ts_platform_data *pdata)
+{
+	struct device_node *np = dev->of_node;
+	int rc;
+
+	u32 temp_val;
+
+	pdata->name = "awinic tech.";
+	rc = of_property_read_string(np, "awin,name", &pdata->name);
+	if (rc && (rc != -EINVAL)) {
+		dev_err(dev, "Unable to read name\n");
+		return rc;
+	}
+
+	/* reset, irq gpio info */
+	pdata->reset_gpio = of_get_named_gpio_flags(np, "awin,reset-gpio",
+			0,
+			&pdata->reset_gpio_flags);
+	if (pdata->reset_gpio < 0)
+		return pdata->reset_gpio;
+
+	pdata->irq_gpio = of_get_named_gpio_flags(np, "awin,irq-gpio",
+			0, &pdata->irq_gpio_flags);
+	if (pdata->irq_gpio < 0)
+		return pdata->irq_gpio;
+	rc = of_property_read_u32(np, "awin,soft-reset-delay-ms", &temp_val);
+	if (!rc)
+		pdata->soft_rst_dly = temp_val;
+	else
+		return rc;
+
+	rc = of_property_read_u32(np, "awin,num-max-touches", &temp_val);
+	if (!rc)
+		pdata->num_max_touches = temp_val;
+	else
+		return rc;
+
+	pdata->fw_vkey_support = of_property_read_bool(np,
+			"awin,fw-vkey-support");
+	return 0;
+}
