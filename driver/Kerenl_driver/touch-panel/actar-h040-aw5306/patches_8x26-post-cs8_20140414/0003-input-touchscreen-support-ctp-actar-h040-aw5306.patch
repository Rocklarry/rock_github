+	len += snprintf(buf+len, PAGE_SIZE-len,
+			"*****AW5x0x Calibrate data*****\n");
+	len += snprintf(buf+len, PAGE_SIZE-len, "TXOFFSET:");
+
+	for (i = 0; i < 11; i++) {
+		len += snprintf(buf+len, PAGE_SIZE-len, "0x%02X ",
