index 32c6f91..70a1e1d 100644
--- a/arch/arm64/configs/msm_defconfig
+++ b/arch/arm64/configs/msm_defconfig
@@ -201,7 +201,8 @@ CONFIG_CMA=y
 CONFIG_CMA_SIZE_MBYTES=64
