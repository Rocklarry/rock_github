+				case C_BMG160_BW_230Hz_U8X:
+					if (duration >
+					C_BMG160_4ms_AutoSleepDur_U8X)
+						v_autosleepduration_u8r =
+						duration;
+					else
+						v_autosleepduration_u8r =
+						C_BMG160_4ms_AutoSleepDur_U8X;
