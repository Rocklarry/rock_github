From b6f7f983874e870b6f0ca37fbb3a84730441d54b Mon Sep 17 00:00:00 2001
From: chenx <chenxiang0527@thundersoft.com>
Date: Wed, 2 Jul 2014 10:11:09 +0800
Subject: [PATCH 2/3] ARM:dts:msm: support stk3310 devicetree

Devicetree support stk3310

Change-Id: I64500f1603268fbc8783b8695a4830c2c0fedb4c
Signed-off-by: Xiang Chen <chenxiang0527@thundersoft.com>
---
 arch/arm/boot/dts/qcom/msm8916-qrd-skuh.dtsi |   20 ++++++++++++++++++++
 1 file changed, 20 insertions(+)

diff --git a/arch/arm/boot/dts/qcom/msm8916-qrd-skuh.dtsi b/arch/arm/boot/dts/qcom/msm8916-qrd-skuh.dtsi
index af488ab..cfbd7ca 100644
--- a/arch/arm/boot/dts/qcom/msm8916-qrd-skuh.dtsi
+++ b/arch/arm/boot/dts/qcom/msm8916-qrd-skuh.dtsi
@@ -88,6 +88,7 @@
 		akm,auto-report;
 	};
 
+/*
 	avago@39 {
 		compatible = "avago,apds9930";
 		reg = <0x39>;
@@ -108,6 +109,25 @@
 		avago,als-D = <129>;
 		avago,ga-value = <98>;
 	};
+*/
+	stk@48 { /* Ambient light and proximity sensor */
+		compatible = "stk,stk3x1x";
+		reg = <0x48>;
+		interrupt-parent = <&msm_gpio>;
+		interrupts = <113 0x2>;
+		vdd-supply = <&pm8916_l17>;
+		vio-supply = <&pm8916_l6>;
+		stk,irq-gpio = <&msm_gpio 113 0x02>;
+		stk,transmittance = <2600>;
+		stk,state-reg = <0x00>;
+		stk,psctrl-reg = <0x31>;
+		stk,alsctrl-reg = <0x29>;
+		stk,ledctrl-reg = <0xFF>;
+		stk,wait-reg = <0x09>;
+		stk,ps-thdh = <100>;
+		stk,ps-thdl = <50>;
+		stk,use-fir;
+	};
 
 	st@18 {
 		compatible = "st,lis3dh";
-- 
