	lt8912@48 {
		compatible = "qcom,lt8912";
		reg = <0x48>;
		pinctrl-names = "default","sleep";
	        pinctrl-0 = <&lt_default>;
	        pinctrl-1 = <&lt_sleep>;
		lt,gpio_rstn = <&tlmm 61 0x0>;
	};	
	
};


